Model :
    sections += Section
    tracks *= Track
;

Track :
    'Track' name=ID ':' sections += [Section]
;

Section :
    'Section' name=ID ':' bars += Bar
;

Bar :
     ticks+=Tick note=ID
;

Tick :
    '.' Separator Tick |
    'x' Separator Tick |
    '.' Tick |
    'x' Tick |
    '.' | 'x'
;

Separator :
    '|'
;