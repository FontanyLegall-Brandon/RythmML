Model :
    'BPM' bpm=INT
    patterns += Pattern
    bars += Bar
    sections += Section
    track = Track
;

Bar :
    'Bar' name=ID 'with'
;

Section :
    'Section' name=ID '{' bars+=[Bar] '}'
 ;

Pattern :
    "#pattern" name=ID beatPattern=BeatPattern
;

BeatPattern :
    '.' Separator BeatPattern |
    '.' BeatPattern |
    '.'
;

Separator :
    '|'
;

Track :
    'Track' name=ID '{' sections+=[Section] '}'
;