RythmML :
    'BPM' bpm=[bpm]
    patterns += Pattern
    bars += Bar
    sections += Section
    track = track
;

Pattern :
    "#pattern" name=ID beats+=Beat

;

Section :
    'Section' name=ID ':' bars+=Bar |
    'Section' name=ID ':' 'REPEAT' bars+=Bar
;

Track :
    'Track' name=ID ':' sections+=Section
;

Bar :
    'Bar' name=ID 'with' pattern=ID ':' beats+=Beat
;


Beat :
     ticks+=Tick note=STRING |
     ticks+=Tick
;

Tick :
    '.' | 'x' | Separator
;

Separator :
    '|'
;


